{
  "name": "notion-assignment-import",
  "version": "4.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesNZL/notion-assignment-import.git"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/webextension-polyfill": "^0.9.0",
    "browserify": "^17.0.0",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "standard-version": "^9.5.0",
    "tinyify": "^3.1.0",
    "tsify": "^5.0.4",
    "typescript": "^4.7.4",
    "vinyl-source-stream": "^2.0.0",
    "webextension-polyfill": "^0.9.0",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "@notionhq/client": "^1.0.4",
    "chrono-node": "^2.3.8"
  },
  "scripts": {
    "debug": "gulp --debug=true",
    "build": "gulp --debug=false",
    "release": "standard-version --sign --commit-all",
    "publish": "git push --follow-tags origin main"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm update -D",
      "postbump": "npm run build && gulp release && git add releases"
    },
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      },
      {
        "filename": "package-lock.json",
        "type": "json"
      },
      {
        "filename": "src/manifest-chromium.json",
        "type": "json"
      },
      {
        "filename": "src/manifest-firefox.json",
        "type": "json"
      }
    ]
  }
}
