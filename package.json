{
  "name": "notion-assignment-import",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesNZL/notion-assignment-import.git"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.179",
    "@types/node": "^17.0.17",
    "browserify": "^17.0.0",
    "gulp": "^4.0.2",
    "standard-version": "^9.3.2",
    "tinyify": "^3.0.0",
    "tsify": "^5.0.4",
    "typescript": "^4.5.5",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "@notionhq/client": "^0.4.13",
    "chrono-node": "^2.3.6"
  },
  "scripts": {
    "build": "tsc & npm run browserify",
    "browserify": "browserify -p tinyify dist/extension.js -o dist/extension.js & browserify -p tinyify dist/parseAssignments.js -o dist/parseAssignments.js & browserify -p tinyify dist/options.js -o dist/options.js",
    "release": "standard-version --sign"
  },
  "standard-version": {
    "bumpFiles": [
      {
        "filename": "package.json",
        "type": "json"
      },
      {
        "filename": "package-lock.json",
        "type": "json"
      },
      {
        "filename": "manifest.json",
        "type": "json"
      }
    ]
  }
}
