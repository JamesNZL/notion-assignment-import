{
	"name": "Notion Canvas Assignment Import",
	"description": "Automatically import Canvas assignments into a Notion database.",
	"version": "2.1",
	"manifest_version": 3,
	"options_ui": {
		"page": "dist/options.html",
		"open_in_tab": true
	},
	"action": {
		"default_popup": "dist/popup.html",
		"default_icon": {
			"16": "assets/favicon/favicon-16x16.png",
			"32": "assets/favicon/favicon-32x32.png",
			"48": "assets/favicon/favicon-48x48.png",
			"128": "assets/favicon/favicon-128x128.png"
		}
	},
	"permissions": [
		"storage",
		"activeTab",
		"scripting"
	],
	"host_permissions": [
		"https://api.notion.com/*"
	],
	"icons": {
		"16": "assets/favicon/favicon-16x16.png",
		"32": "assets/favicon/favicon-32x32.png",
		"48": "assets/favicon/favicon-48x48.png",
		"128": "assets/favicon/favicon-128x128.png"
	}
}